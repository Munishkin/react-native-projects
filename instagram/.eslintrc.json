{
    "env": {
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-console": "off",
        "no-use-before-define": [
            "error",
            {
                "variables": false
            }
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "prettier/prettier": [
            "error"
        ]
    }
}